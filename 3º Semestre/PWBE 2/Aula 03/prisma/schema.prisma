
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}

model Carros {
  id Int @id @default(autoincrement())
  placa String
  marca String
  modelo String
  ano_veiculo Int 
}

model Clientes {
  id Int @id @default(autoincrement())
  nome_completo String
  CPF String
  email String
  CNH String
}